import java.util.Scanner;

public class Ordem {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		Scanner  in = new Scanner(System.in);

		
		int a[]= new int [10];
		int p=0;
		
		 int b[]=new int[10];
		for(int i =a.length-1;i>=0;i--){
			p++;
			System.out.println(p+"  digite os valores");
			a[i] = in.nextInt();
			b[i]=a[i];
		}
		
		for(int j = 0;j<=10;j++){
		    System.out.println("valor de b:  "+b[j]);

			
		}
		
	}
	
}
